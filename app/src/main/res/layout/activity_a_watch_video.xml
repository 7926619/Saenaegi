<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".aLfreeMainActivity"
    android:importantForAccessibility="no">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_weight="1"
        android:importantForAccessibility="no">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:background="#FFF"
            android:importantForAccessibility="no">

            <TextView
                android:id="@+id/textView9"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:adjustViewBounds="true"
                android:textSize="25sp"
                android:textColor="#363636"
                android:textStyle="bold"
                android:contentDescription="영상 시청"
                android:focusable="true"
                android:focusableInTouchMode="true"
                android:nextFocusDown="@+id/textView22"
                android:nextFocusUp="@+id/textView5"
                android:accessibilityTraversalAfter="@+id/textView22"
                android:accessibilityTraversalBefore="@+id/textView5"
                android:importantForAccessibility="yes"
                android:text="영상 시청" />

            <View
                android:id="@+id/view"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_alignParentBottom="true"
                android:layout_marginTop="20dp"
                android:background="#d5d5d5" />

        </RelativeLayout>

        <view
            android:id="@+id/youtube_screen"
            class="com.google.android.youtube.player.YouTubePlayerView"
            id="@+id/view2"
            android:layout_width="match_parent"
            android:layout_height="110dp"
            android:importantForAccessibility="no"
            />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:paddingHorizontal="30dp"
            android:paddingTop="20dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView22"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textSize="20sp"
                    android:gravity="center"
                    android:textColor="#363636"
                    android:contentDescription="재생"
                    android:text="재생"
                    android:focusable="true"
                    android:focusableInTouchMode="true"
                    android:nextFocusDown="@+id/textView19"
                    android:nextFocusUp="@+id/textView5"
                    android:accessibilityTraversalAfter="@+id/textView19"
                    android:accessibilityTraversalBefore="@+id/textView5"
                    android:importantForAccessibility="yes"/>

                <TextView
                    android:id="@+id/textView19"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textSize="20sp"
                    android:gravity="center"
                    android:textColor="#363636"
                    android:contentDescription="정지"
                    android:text="정지"
                    android:focusable="true"
                    android:focusableInTouchMode="true"
                    android:nextFocusDown="@+id/textView21"
                    android:nextFocusUp="@+id/textView22"
                    android:accessibilityTraversalAfter="@+id/textView21"
                    android:accessibilityTraversalBefore="@+id/textView22"
                    android:importantForAccessibility="yes"/>

                <TextView
                    android:id="@+id/textView21"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textSize="20sp"
                    android:gravity="center"
                    android:textColor="#363636"
                    android:contentDescription="10초 앞으로"
                    android:text=" 10초 앞으로"
                    android:focusable="true"
                    android:focusableInTouchMode="true"
                    android:nextFocusDown="@+id/textView20"
                    android:nextFocusUp="@+id/textView19"
                    android:accessibilityTraversalAfter="@+id/textView20"
                    android:accessibilityTraversalBefore="@+id/textView19"
                    android:importantForAccessibility="yes"/>

                <TextView
                    android:id="@+id/textView20"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textSize="20sp"
                    android:gravity="center"
                    android:textColor="#363636"
                    android:contentDescription="10초 뒤로"
                    android:text=" 10초 뒤로"
                    android:focusable="true"
                    android:focusableInTouchMode="true"
                    android:nextFocusDown="@+id/textView16"
                    android:nextFocusUp="@+id/textView21"
                    android:accessibilityTraversalAfter="@+id/textView16"
                    android:accessibilityTraversalBefore="@+id/textView21"
                    android:importantForAccessibility="yes"/>

            </LinearLayout>

            <TextView
                android:id="@+id/textView16"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="20sp"
                android:layout_marginBottom="10dp"
                android:textColor="#363636"
                android:contentDescription="파트 선택"
                android:text="파트 선택"
                android:focusable="true"
                android:focusableInTouchMode="true"
                android:nextFocusDown="@+id/textView"
                android:nextFocusUp="@+id/textView20"
                android:accessibilityTraversalAfter="@+id/textView"
                android:accessibilityTraversalBefore="@+id/textView20"
                android:importantForAccessibility="yes"/>

            <TextView
                android:id="@+id/textView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:contentDescription="같은 파트에 다른 것 선택"
                android:text="같은 파트에 다른 것 선택"
                android:textColor="#363636"
                android:textSize="20sp"
                android:focusable="true"
                android:focusableInTouchMode="true"
                android:nextFocusDown="@+id/textView15"
                android:nextFocusUp="@+id/textView16"
                android:accessibilityTraversalAfter="@+id/textView15"
                android:accessibilityTraversalBefore="@+id/textView16"
                android:importantForAccessibility="yes"/>

            <TextView
                android:id="@+id/textView15"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="20sp"
                android:layout_marginBottom="10dp"
                android:textColor="#363636"
                android:contentDescription="추천하기"
                android:text="추천하기"
                android:focusable="true"
                android:focusableInTouchMode="true"
                android:nextFocusDown="@+id/textView5"
                android:nextFocusUp="@+id/textView"
                android:accessibilityTraversalAfter="@+id/textView5"
                android:accessibilityTraversalBefore="@+id/textView"
                android:importantForAccessibility="yes" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:textColor="#363636"
                android:textSize="20sp"
                android:text="신고하기"
                android:contentDescription="신고하기"
                android:focusable="true"
                android:focusableInTouchMode="true"
                android:nextFocusDown="@+id/textView22"
                android:nextFocusUp="@+id/textView15"
                android:accessibilityTraversalAfter="@+id/textView22"
                android:accessibilityTraversalBefore="@+id/textView15"
                android:importantForAccessibility="yes"/>

        </LinearLayout>
    </LinearLayout>

</LinearLayout>